
*.bat text eol=crlf executable core.fileMode=true
*.cmd text eol=crlf executable core.fileMode=true
*.sh text eol=lf executable core.fileMode=true
*.java text eol=lf -executable
*.scala text eol=lf -executable
*.xml text eol=lf -executable
*.py text eol=lf -executable
*.R text eol=lf -executable
# mimicking apache spark

# Auto detect text files and perform LF normalization
* text=auto eol=lf -executable
* -executable